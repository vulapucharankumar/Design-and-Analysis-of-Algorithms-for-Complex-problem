#include <stdio.h>

int main() {
   int array[10] = {16, 22, 38, 84, 13, 06, 57, 89, 98, 12};
   int loop, largest;

   largest = array[0];
   
   for(loop = 1; loop < 10; loop++) {
      if( largest < array[loop] ) 
         largest = array[loop];
   }
   
   printf("Largest element of array is %d", largest);   
   
   return 0;
}
